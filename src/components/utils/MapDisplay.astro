---
export interface Props {
  mapQuery: string;
}

const { mapQuery } = Astro.props;

const googleLink = "https://www.google.com/maps/embed/v1/place?key=AIzaSyCD3FjE5qT-mt3AcJuvktSAzBrEWBpURxU&q=" + mapQuery + "&zoom=18&maptype=satellite";
---

<iframe
  width="720"
  height="450"
  style="border:0"
  loading="lazy"
  allowfullscreen
  referrerpolicy="no-referrer-when-downgrade"
  src={googleLink}>
</iframe>